main {
  padding: 100px;
  margin-left: auto;
  margin-right: auto;
  background-color: #BF0000;
  overflow: hidden;
}

caption {
  font-family: 'Poppins', sans-serif;
  font-size: 2em;
  font-weight: 700;
}

table {
  border: 1px solid #000;
  font-family: 'Poppins', sans-serif;
  border-collapse: collapse;
}

th {
  text-align: left;
  padding: 15px;
  font-weight: 900;
  border: 1px solid #000;
}

td {
  padding: 15px;
  text-align: left;
  border: 1px solid #000;
}

.tab-font1 {
  font-size: 1.5em;
  color: darkcyan;
  text-align: center;
}
 
.tab-font2 {
  font-size: 1.5em;
  color: #BF0000;
  text-align: center;
}


.tab-font2:nth-child(1) {
  background-color: rgb(0,15,97);
}

.tab-font2:nth-child(2) {
  background-color: rgb(0,15,97);
}

.tab-font2:nth-child(3) {
  background-color: rgb(0,15,97);
}

.tab-font2:nth-child(4) {
  background-color: rgb(0,15,97);
}
.tab-font2:nth-child(5) {
  background-color: rgb(0,15,97);
}
.tab-font2:nth-child(6) {
  background-color: rgb(0,15,97);
}